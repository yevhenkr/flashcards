    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@it-incubator/eslint-config": "^1.0.1",
    "@it-incubator/prettier-config": "^0.1.2",
    "@it-incubator/stylelint-config": "^0.1.5",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "sass": "^1.69.5",
    "stylelint": "^16.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
